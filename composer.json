{
  "name": "flugger/laravel-responder",
  "description": "A Laravel package for building API responses.",
  "keywords": ["laravel", "lumen", "api", "response"],
  "homepage": "https://github.com/flugger/laravel-responder",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Tømmerås",
      "email": "flugged@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "laravel/framework": "^6.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Flugg\\Responder\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Flugg\\Responder\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Flugg\\Responder\\ResponderServiceProvider"
      ]
    }
  }
}
