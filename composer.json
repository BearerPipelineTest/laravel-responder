{
    "name": "flugger/laravel-responder",
    "description": "A Laravel package for building API responses.",
    "keywords": [
        "laravel",
        "lumen",
        "responder",
        "response",
        "api",
        "json-api"
    ],
    "homepage": "https://github.com/flugger/laravel-responder",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Tømmerås",
            "email": "flugged@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/testing": "^7.0|^8.0",
        "illuminate/validation": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Flugg\\Responder\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flugg\\Responder\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flugg\\Responder\\ResponderServiceProvider"
            ],
            "aliases": {
                "Responder": "Flugg\\Responder\\Facades\\Responder"
            }
        }
    }
}
